{
  "name": "Auto Scroll",
  "version": "0.2",
  "manifest_version": 3,
  "description": "Scrolls down automatically. Click twice for stop scrolling.",
  "background": {
    "service_worker": "service_worker.js"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {},
  "content_security_policy": {}
}