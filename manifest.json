{
  "name": "Auto Scroll",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Scrolls down automatically",
  "background": {
    "service_worker": "service_worker.js"
  },
  "permissions": [
    "activeTab"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "action": {},
  "content_security_policy": {}
}